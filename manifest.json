{
  "name": "Gmail Assistant",
  "version": "1.0",
  "description": "An assistant that helps you navigate Gmail.",
  "key":"",
  "oauth2": {
  "client_id": "240312093323-91d8t3ms8aish2ljr4fr0h97chp297sb.apps.googleusercontent.com",
  "scopes":["https://www.googleapis.com/auth/gmail.readonly",
  "https://www.googleapis.com/auth/gmail.labels",
  "https://www.googleapis.com/auth/gmail.modify",
  "https://www.googleapis.com/auth/contacts.readonly",
  "https://www.googleapis.com/auth/gmail.send",
  "https://www.googleapis.com/auth/calendar"]
},
"host_permissions": [
  "https://*.googleapis.com/",
  "https://mail.google.com/"
],
  "manifest_version": 3,
  "action": {
    "default_title": "Gmail Assistant"
  },
  "permissions": [
  "identity","sidePanel","storage","scripting"
],
  "side_panel":{
    "default_path":"index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"],
      "css": [
        "css/gmail-content.css"
      ],
      "js": [
        "js/gmail-content.js"
      ]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["oauth.js"]
    }
  ]
}